	],
	"targetPath": "build/libraries/",
	"buildRequirements": [
		"silenceWarnings",
		"silenceDeprecations",
		"requireBoundsCheck"
	],
	"dependencies": {
		"asn1": "~>2.2.0",
		"csprng": "~>1.1.0"
	}
}