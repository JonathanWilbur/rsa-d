{
    "name": "rsa",
    "description": "Rivest-Shamir-Adleman asymmetric cryptography",
    "authors": [
        "Jonathan M. Wilbur <jonathan@wilbur.space>"
    ],
    "license": "MIT",
	"homepage": "https://github.com/JonathanWilbur/rsa-d",
	"targetType": "executable",
	"importPaths": [
		"source/"
	],
	"sourcePaths": [
